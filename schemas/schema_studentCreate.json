{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["firstName", "familyName", "dob", "email"],
    "properties": {
        "firstName": {
            "type": "string"
        },
        "familyName": {
            "type": "string"
        },
        "dob": {
            "type": "string",
            "format": "date"
        },
        "email": {
            "type": "string",
            "format": "email"
        }
    },
    "additionalProperties": false
}
